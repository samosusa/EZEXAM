<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="TOP_CENTER" prefHeight="220.0" prefWidth="471.0" spacing="10.0" userData="Temperature presenter" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="temperature.view.TemperatureViewController">
   <children>
      <HBox>
         <children>
            <Label alignment="CENTER" prefHeight="35.0" prefWidth="597.0" text="Temperature presenter" textAlignment="CENTER">
               <font>
                  <Font size="24.0" />
               </font>
            </Label>
         </children>
      </HBox>
      <HBox alignment="CENTER" prefHeight="87.0" prefWidth="471.0" spacing="20.0">
         <children>
            <Button fx:id="updateButton" mnemonicParsing="false" onAction="#updateButtonPressed" prefHeight="51.0" prefWidth="114.0" text="Update" textAlignment="CENTER">
               <font>
                  <Font size="24.0" />
               </font>
            </Button>
            <Label fx:id="filterLabel" alignment="CENTER_RIGHT" prefHeight="39.0" prefWidth="89.0" text="All" textAlignment="RIGHT">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <TextField fx:id="filterField" prefHeight="39.0" prefWidth="84.0">
               <font>
                  <Font size="18.0" />
               </font>
            </TextField>
            <Button fx:id="filterButton" mnemonicParsing="false" onAction="#onFilter" text="Filter">
               <font>
                  <Font size="18.0" />
               </font>
            </Button>
         </children>
         <padding>
            <Insets left="10.0" right="10.0" />
         </padding>
      </HBox>
      <Label fx:id="outputLabel" alignment="CENTER" prefHeight="35.0" prefWidth="597.0" textAlignment="CENTER" VBox.vgrow="ALWAYS">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
   </children>
</VBox>
